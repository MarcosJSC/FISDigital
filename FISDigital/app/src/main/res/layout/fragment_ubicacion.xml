<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="cr.bamboo.fisdigital.layout.activity.Activity_Ubicacion$PlaceholderFragment">
<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:visibility="gone"/>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/svGeneral">
        <GridLayout
            android:columnCount="1"
            android:rowCount="20"
            style="@style/FISWrapperGrid">
            <LinearLayout
                android:id="@+id/errorMsg"
                android:visibility="gone"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/error_msg"
                >
                <GridLayout
                    android:layout_marginLeft="260dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="12dip"
                    android:paddingBottom="12dip"
                    android:layout_gravity="center"
                    android:orientation="horizontal"
                    >
                    <ImageView
                        android:layout_width="28dip"
                        android:layout_height="28dip"
                        android:background="@drawable/alert"/>
                    <TextView
                        android:textColor="#FFF"
                        android:layout_marginLeft="6dip"
                        android:layout_marginTop="4dip"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Revise los campos en rojo"/>

                    </GridLayout>

        </LinearLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(1) 1. Número de Folio "
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle"
                    android:focusableInTouchMode="true"
                    android:focusable="true">
                    <requestFocus></requestFocus>
                    </TextView>

                <EditText
                    android:id="@+id/etNumeroFolio"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:inputType="number"
                    android:imeOptions="actionDone"
                    style="@style/FISEditTextMedium" />

            </GridLayout>

            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="2. Georeferenciación"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <GridLayout
                    android:layout_row="1"
                    android:layout_column="0"
                    android:columnCount="4"
                    android:rowCount="2"
                    style="@style/FISRowGrid">

                    <TextView
                        android:text="Latitud"
                        android:layout_row="0"
                        android:layout_column="0"
                        style="@style/FISTextViewTitleNested" />

                    <EditText
                        android:id="@+id/etLatitud"
                        android:layout_row="0"
                        android:layout_column="1"
                        style="@style/FISEditTextMediumLeftMargin"
                        android:enabled="false" />

                    <TextView
                        android:text="Longitud"
                        android:layout_row="0"
                        android:layout_column="2"
                        android:layout_marginLeft="40dp"
                        style="@style/FISTextViewTitleNested" />

                    <EditText
                        android:id="@+id/etLongitud"
                        android:layout_row="0"
                        android:layout_column="3"
                        style="@style/FISEditTextMediumLeftMargin"
                        android:enabled="false"
                        />
                </GridLayout>

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(5) 3. Provincia"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvProvincia"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView"/>

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnProvincia"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(6) 4. Cantón"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvCanton"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnCanton"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(7) 5. Distrito"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvDistrito"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnDistrito"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(8) 6. Barrio"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvBarrio"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnBarrio"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(9) 7. Caserio"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvCaserio"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnCaserio"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="8. Region"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />


                <TextView
                    android:id="@+id/tvRegion"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:enabled="false"
                    style="@style/FISTextView" />


            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="9. ARDS"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle"/>

                <TextView
                    android:id="@+id/tvGerenciaRegional"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:enabled="false"
                    style="@style/FISTextView" />

            </GridLayout>

            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(10) 10. Zona"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvZona"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnZona"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(11) 11. Dirección Exacta"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <EditText
                    android:id="@+id/etDireccionExacta"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:inputType="textMultiLine"
                    android:imeOptions="actionDone"
                    style="@style/FISEditTextArea" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(12) 12. Número de vivienda"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <EditText
                    android:id="@+id/etNumeroVivienda"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:imeOptions="actionDone"
                    style="@style/FISEditTextMedium" />

            </GridLayout>
            <GridLayout
                android:columnCount="2"
                android:rowCount="2"
                style="@style/FISRowGrid">

                <TextView
                    android:text="(13) 13. Vivienda en riesgo"
                    android:layout_row="0"
                    android:layout_column="0"
                    style="@style/FISTextViewTitle" />

                <TextView
                    android:id="@+id/tvViviendaRiesgo"
                    android:layout_row="1"
                    android:layout_column="0"
                    style="@style/FISTextView" />

                <Button
                    android:text="+"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:id="@+id/btnViviendaRiesgo"
                    style="@style/FISButtonCatalogo" />

            </GridLayout>
            <GridLayout style="@style/FISRowGrid">
                <Button
                    android:text="SIGUIENTE"
                    android:id="@+id/btnSiguiente"
                    style="@style/FISButtonSiguiente" />
            </GridLayout>
        </GridLayout>
    </ScrollView>

</LinearLayout>
